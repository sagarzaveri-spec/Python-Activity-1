import random
Harsh_number_list=[1,3,5,6,7,22,167,432,43]
Random_Number=random.choice(Harsh_number_list)
print(f"Random number selected:",Random_Number)
Harsh_Guess=int(input("Harsh's Guess: "))
if Random_Number<Harsh_Guess:
    print("The guessed number is larger than the original")
elif Random_Number>Harsh_Guess:
    print("The guessed number is smaller than the original")
else:
    print("You have guessed the correct number")
